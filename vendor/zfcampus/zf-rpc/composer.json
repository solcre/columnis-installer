{
    "name": "zfcampus/zf-rpc",
    "description": "ZF2 Module for simplifying the creation of RPC services",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "zend",
        "module",
        "rpc"
    ],
    "homepage": "http://apigility.org/",
    "support": {
        "email": "apigility-users@zend.com",
        "irc": "irc://irc.freenode.net/apigility",
        "source": "https://github.com/zfcampus/zf-rpc",
        "issues": "https://github.com/zfcampus/zf-rpc/issues"
    },
    "repositories": [
        { "type": "composer", "url": "https://packages.zendframework.com" }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    },
    "require": {
        "php": ">=5.3.23",
        "zendframework/zend-eventmanager": "~2.3",
        "zendframework/zend-http": "~2.3",
        "zendframework/zend-mvc": "~2.3",
        "zendframework/zend-servicemanager": "~2.3",
        "zendframework/zend-stdlib": "~2.3",
        "zendframework/zend-view": "~2.3",
        "zfcampus/zf-api-problem": "~1.0",
        "zfcampus/zf-content-negotiation": "~1.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "*@dev"
    },
    "autoload": {
        "psr-4": {
            "ZF\\Rpc\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    }
}
